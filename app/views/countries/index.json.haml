:erb
  {
    "cols": [
        {
            "id": 0,
            "label": "Country",
            "type": "string"
        },
        {
            "id": 1,
            "label": "Volunteers",
            "type": "number"
        }
    ],
    "rows": [
        <% for country in Country.unscoped.order('name ASC').all do %>
          {
            "c": [
                {
                    "v": "<%= country.name %>"
                },
                {
                    "v": <%= country.num_volunteers.to_i %>
                }
            ]
          }<%= ',' unless country == Country.last %>
        <% end %>
    ]
  }
