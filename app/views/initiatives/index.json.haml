:erb
  {
    "cols": [
        {
            "id": 0,
            "label": "Country",
            "type": "string"
        },
        {
            "id": 1,
            "label": "Initiatives",
            "type": "number"
        }
    ],
    "rows": [
      <% for country in @countries do %>
        {
          "c": [
              {
                  "v": "<%= country.name %>"
              },
              {
                  "v": <%= country.initiatives.count %>
              }
          ]
        }<%= ',' unless country == @countries.last %>
      <% end %>
    ]
  }
